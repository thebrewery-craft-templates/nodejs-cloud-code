{
  "name": "my-dev-parse-server",
  "version": "0.1.0",
  "description": "My Development Parse Server with Cloud Code",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParsePlatform/parse-server.git"
  },
  "license": "MIT",
  "dependencies": {
    "@brewery/parse-job-scheduler": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "newrelic": "^6.13.0",
    "parse": "^2.19.0",
    "parse-dashboard": "^2.1.0",
    "parse-server": "^4.5.0",
    "parse-server-email-template-adapter": "^1.0.2"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "babel-eslint": "10.1.0",
    "babel-watch": "7.4.0",
    "eslint": "7.19.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "jasmine": "3.6.4",
    "nyc": "15.1.0",
    "nodemon": "^1.9.2"
  },
  "scripts": {
    "dev": "concurrently --kill-others 'npm run server'",
    "server": "VERBOSE=1 nodemon -r esm ./index.js",
    "start": "nodemon -r esm ./index.js",
    "lint": "eslint --cache ./cloud && eslint --cache index.js && eslint --cache ./spec",
    "lint-fix": "eslint --cache --fix ./cloud && eslint --cache --fix index.js && eslint --cache  --fix./spec",
    "test": "node -r esm ./node_modules/jasmine/bin/jasmine",
    "coverage": "node -r esm ./node_modules/nyc/bin/nyc node -r esm ./node_modules/jasmine/bin/jasmine"
  },
  "engines": {
    "node": ">=4.3"
  }
}
